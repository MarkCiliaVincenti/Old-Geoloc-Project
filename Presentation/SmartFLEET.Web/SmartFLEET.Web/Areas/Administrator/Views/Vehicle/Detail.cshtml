        <div class="col-lg-6">
            <h3>Détail véhicule</h3>

            <dl class="dl-horizontal">
                <dt>
                    VehicleName
                </dt>

                <dd>
                    {{vehicle.VehicleName}}
                </dd>
                <br />
                <dt>
                    LicensePlate
                </dt>

                <dd>
                    {{vehicle.LicensePlate}}
                </dd>
                <br />
                <dt>
                    VIN
                </dt>

                <dd>
                    {{vehicle.Vin}}
                </dd>
                <br />
